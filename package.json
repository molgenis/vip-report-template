{
  "name": "@molgenis/vip-report-template",
  "version": "5.7.0",
  "description": "Report Template for Variant Call Format (VCF) Report Generator",
  "scripts": {
    "build": "vite build",
    "build-dev": "vite build --mode dev",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format": "prettier --write src/**/*.{tsx,ts}",
    "lint": "eslint src/**/*.{tsx,ts}",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "license": "LGPL-3.0",
  "devDependencies": {
    "@molgenis/vite-plugin-inline": "^1.0.19",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.1",
    "bulma": "^0.9.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-solid": "^0.13.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.6",
    "vite-plugin-solid": "^2.7.2",
    "vitest": "^1.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@molgenis/vip-report-api": "^4.7.0",
    "@molgenis/vip-report-vcf": "^1.7.0",
    "@solidjs/router": "^0.9.1",
    "base64-js": "^1.5.1",
    "igv": "link:..\\igv.js",
    "solid-js": "^1.8.7"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint",
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/molgenis/vip-report-template/issues"
  },
  "homepage": "https://github.com/molgenis/vip-report-template#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molgenis/vip-report-template.git"
  }
}
