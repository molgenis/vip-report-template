    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.1.0",
    "bulma": "^0.9.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "prettier": "2.5.1",
    "rollup": "^2.66.1",
    "sass": "^1.49.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
